{
  "version": 2,
  "builds": [
    {
      "src": "requirements.txt", 
      "use": "@vercel/python@3.9"
    },
    {
      "src": "coupon_manager/wsgi.py",
      "use": "@vercel/python@3.9",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "coupon_manager/wsgi.py"
    }
  ]
}
