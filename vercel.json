{
  "version": 2,
  "builds": [
    {
      "src": "coupon_manager/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "cmd": "python -m pip install -r requirements.txt",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "coupon_manager/wsgi.py"
    }
  ]
}
